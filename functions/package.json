{
  "name": "JoinAPI",
  "author": "Johnny Ray Alt <johnnyrayalt@gmail.com>",
  "version": "1.0.0",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -p tslint.json",
    "serve:dev": "export NODE_ENV=development && ./node_modules/.bin/tsc && firebase -p 7777 serve --only functions",
    "serve": "firebase -p 7777 serve --only functions",
    "serve:debug": "firebase -p 7777 serve --debug --only functions",
    "build:dev": "export NODE_ENV=development && ./node_modules/.bin/tsc",
    "build:prod": "export NODE_ENV=production && ./node_modules/.bin/tsc",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@google-cloud/firestore": "^0.17.0",
    "@google-cloud/functions-emulator": "^1.0.0-beta.5",
    "@types/core-js": "^2.5.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.7.1",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-minify": "^1.0.0",
    "firebase-admin": "^7.4.0",
    "firebase-functions": "^2.0.5",
    "helmet": "^3.14.0",
    "morgan": "^1.9.0",
    "request": "^2.79.0",
    "router": "^1.3.3",
    "systemjs": "^2.0.2",
    "ts-interface-checker": "^0.1.4",
    "ts-runtime": "^0.1.35",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2"
    
  },
  "devDependencies": {
    "@types/node": "^10.11.7",
    "eslint": "^4.12.0",
    "eslint-plugin-promise": "^3.6.0",
    "ts-interface-builder": "^0.1.5",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "private": true
}
